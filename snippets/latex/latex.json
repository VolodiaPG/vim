{
    "Basic Document": {
        "prefix": "doc",
        "body": [
            "\\documentclass{article}",
            "\\usepackage[utf8]{inputenc}",
            "\\usepackage[T1]{fontenc}",
            "\\usepackage{amsmath}",
            "\\usepackage{amsfonts}",
            "\\usepackage{amssymb}",
            "\\usepackage{graphicx}",
            "\\usepackage{hyperref}",
            "",
            "\\title{${1:title}}",
            "\\author{${2:author}}",
            "\\date{\\today}",
            "",
            "\\begin{document}",
            "",
            "\\maketitle",
            "",
            "${0}",
            "",
            "\\end{document}"
        ],
        "description": "Basic LaTeX document structure"
    },
    "Figure": {
        "prefix": "fig",
        "body": [
            "\\begin{figure}[${1:htbp}]",
            "    \\centering",
            "    \\includegraphics[width=${2:0.8}\\textwidth]{${3:filename}}",
            "    \\caption{${4:caption}}",
            "    \\label{fig:${5:label}}",
            "\\end{figure}"
        ],
        "description": "Figure environment"
    },
    "Table": {
        "prefix": "tab",
        "body": [
            "\\begin{table}[${1:htbp}]",
            "    \\centering",
            "    \\begin{tabular}{${2:c c c}}",
            "        ${3:Header 1} & ${4:Header 2} & ${5:Header 3} \\\\",
            "        \\hline",
            "        ${6:data} & ${7:data} & ${8:data} \\\\",
            "        ${0}",
            "    \\end{tabular}",
            "    \\caption{${9:caption}}",
            "    \\label{tab:${10:label}}",
            "\\end{table}"
        ],
        "description": "Table environment"
    },
    "Equation": {
        "prefix": "eq",
        "body": [
            "\\begin{equation}",
            "    ${1}",
            "    \\label{eq:${2:label}}",
            "\\end{equation}"
        ],
        "description": "Equation environment"
    },
    "Align": {
        "prefix": "align",
        "body": [
            "\\begin{align}",
            "    ${1}",
            "    \\label{eq:${2:label}}",
            "\\end{align}"
        ],
        "description": "Align environment"
    },
    "Itemize": {
        "prefix": "item",
        "body": [
            "\\begin{itemize}",
            "    \\item ${1}",
            "    ${0}",
            "\\end{itemize}"
        ],
        "description": "Itemize environment"
    },
    "Enumerate": {
        "prefix": "enum",
        "body": [
            "\\begin{enumerate}",
            "    \\item ${1}",
            "    ${0}",
            "\\end{enumerate}"
        ],
        "description": "Enumerate environment"
    },
    "Section": {
        "prefix": "sec",
        "body": [
            "\\section{${1:section name}}",
            "${0}"
        ],
        "description": "Section"
    },
    "Subsection": {
        "prefix": "ssec",
        "body": [
            "\\subsection{${1:subsection name}}",
            "${0}"
        ],
        "description": "Subsection"
    },
    "Subsubsection": {
        "prefix": "sssec",
        "body": [
            "\\subsubsection{${1:subsubsection name}}",
            "${0}"
        ],
        "description": "Subsubsection"
    },
    "Math": {
        "prefix": "math",
        "body": [
            "\\begin{math}",
            "    ${1}",
            "\\end{math}"
        ],
        "description": "Inline math environment"
    },
    "Display Math": {
        "prefix": "dmath",
        "body": [
            "\\begin{displaymath}",
            "    ${1}",
            "\\end{displaymath}"
        ],
        "description": "Display math environment"
    },
    "Inline Math": {
        "prefix": "im",
        "body": [
            "$${1}$"
        ],
        "description": "Inline math"
    },
    "Display Inline Math": {
        "prefix": "dm",
        "body": [
            "$$${1}$$"
        ],
        "description": "Display inline math"
    }
}